{
  "name": "sklist",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "postinstall": "ng build --output-path dist",
    "start": "node server.js",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config proxy.conf.json --open\" \"nodemon server.js\"",
    "prod": "concurrently \"mongod\" \"ng build --aot --prod && node server.js\"",
    "test": "ng test",
    "lint": "ng lint",
    "linthtml": "htmlhint \"client/**/*.html\"",
    "lintscss": "sass-lint \"client/**/*.scss\" -v",
    "e2e": "ng e2e"
  },
